<?xml version="1.0" encoding="utf-8"?>
<Map background-color='lightgreen'>

  <Style name="style">
    <Rule>
      <ShieldSymbolizer
        face-name="DejaVu Sans Bold" size="6" fill="#000000"
        file="shield[type].svg">
            [name]
      </ShieldSymbolizer> 
    </Rule>
  </Style>

  <Layer name="layer">
    <StyleName>style</StyleName>
    <Datasource>
      <Parameter name="file">shield.json</Parameter>
      <Parameter name="type">geojson</Parameter>
    </Datasource>
  </Layer>

</Map>

