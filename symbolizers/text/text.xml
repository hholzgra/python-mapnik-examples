<?xml version="1.0" encoding="utf-8"?>
<Map background-color='lightgreen'>

  <Style name="style">
    <Rule>
      <LineSymbolizer stroke="red"/>
      <TextSymbolizer placement="line" face-name="DejaVu Sans Book">[label]</TextSymbolizer>
    </Rule>
  </Style>

  <Layer name="layer">
    <StyleName>style</StyleName>
    <Datasource>
      <Parameter name="file">text.json</Parameter>
      <Parameter name="type">geojson</Parameter>
    </Datasource>
  </Layer>

</Map>

